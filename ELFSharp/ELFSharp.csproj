<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <SignAssembly>False</SignAssembly>
    <AssemblyOriginatorKeyFile>ELFSharp.public.snk</AssemblyOriginatorKeyFile>
    <PackageId>ELFSharp</PackageId>
    <Version>1.0.5</Version>
    <Description>C# library for reading binary ELF, UImage, Mach-O files</Description>
    <Authors>konrad.kruczynski</Authors>
    <PackageLicenseUrl>https://github.com/konrad-kruczynski/elfsharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/konrad-kruczynski/elfsharp/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/konrad-kruczynski/elfsharp/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ELF; UImage; Mach-O</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="MiscUtil\" />
  </ItemGroup>

</Project>
